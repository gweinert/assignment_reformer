<% @user.errors.full_messages.each do |i| %>
<%= "#{i}" %><br>
<% end %>
<br><br>


<%= form_for(@user) do |f| %>



<%= f.text_field(:username) %><br>

<%= get_errors("username", @user) %>

<br>


<%= f.email_field(:email) %><br>

<%= get_errors("email", @user) %>


<%= f.password_field(:password) %>

<%= get_errors("password", @user) %>

<br><%= f.submit("Submit") %><br>

<% end %>
